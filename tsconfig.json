{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "skipDefaultLibCheck": true,
    "noImplicitAny": true
  },
  "exclude": ["./taze.config.ts"]
}
