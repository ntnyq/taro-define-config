{
  "eslint.validate": [
    "vue",
    "yaml",
    "html",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "csso",
    "esbuild",
    "icss",
    "nerv",
    "nocheck",
    "nosources",
    "pnpm",
    "pxtransform",
    "tarojs",
    "taze",
    "weapp"
  ],
  "cSpell.allowCompoundWords": true
}
