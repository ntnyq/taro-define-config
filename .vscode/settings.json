{
  "oxc.enable": true,
  "eslint.enable": true,
  "prettier.enable": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "oxc.oxc-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
    "source.sortImports": "never"
  },
  "eslint.validate": [
    "vue",
    "yaml",
    "toml",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "typescript.experimental.useTsgo": true,
  "cSpell.words": [
    "browserslistrc",
    "bumpp",
    "changelogithub",
    "csso",
    "esbuild",
    "icss",
    "iife",
    "importee",
    "jridgewell",
    "jsxdev",
    "kwai",
    "lfcr",
    "nerv",
    "nextjs",
    "nocheck",
    "nosources",
    "ohpm",
    "oxfmt",
    "pnpm",
    "pxtransform",
    "systemjs",
    "tarojs",
    "taze",
    "tsdown",
    "tsgo",
    "unrs",
    "weapp"
  ],
  "cSpell.allowCompoundWords": true
}
